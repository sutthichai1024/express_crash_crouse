<ul class="restaurants">
    {{#each restaurants }}
    <li class="restaurants-item">
        <img src="{{this.imageURL}}" alt="" class="restaurants-item-image">
        <div class="restaurants-item-image">{{ this.name}}</div>
        <small class="restaurants-item-name"> {{ this.type}}</small>
    </li>
    {{/each}}
</ul>